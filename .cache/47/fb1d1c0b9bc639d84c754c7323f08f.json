enerated":{"line":105,"column":32},"source":"src/records/records.js","original":{"line":64,"column":8}},{"generated":{"line":106,"column":0},"source":"src/records/records.js","original":{"line":66,"column":8}},{"name":"root","generated":{"line":106,"column":11},"source":"src/records/records.js","original":{"line":66,"column":13}},{"generated":{"line":106,"column":15},"source":"src/records/records.js","original":{"line":66,"column":8}},{"name":"appendChild","generated":{"line":106,"column":16},"source":"src/records/records.js","original":{"line":66,"column":18}},{"generated":{"line":106,"column":27},"source":"src/records/records.js","original":{"line":66,"column":8}},{"name":"node","generated":{"line":106,"column":28},"source":"src/records/records.js","original":{"line":66,"column":30}},{"generated":{"line":106,"column":32},"source":"src/records/records.js","original":{"line":66,"column":8}},{"generated":{"line":107,"column":0},"source":"src/records/records.js","original":{"line":68,"column":5}},{"name":"HTMLElement","generated":{"line":111,"column":19},"source":"src/records/records.js","original":{"line":12,"column":31}},{"name":"RecordsContainer","generated":{"line":113,"column":15},"source":"src/records/records.js","original":{"line":72,"column":15}}],"sources":{"src/records/records.js":"const template = document.createElement('template');\r\ntemplate.innerHTML = `\r\n<section id=\"records\">\r\n<head><h2> <span id=\"recordsFound\"></span> records found</h2></head>\r\n    <section id=\"recordsList\"></section>\r\n    <section id=\"sum\">\r\n        <span>Total: </span>\r\n        <span id=\"sumValue\"></span>\r\n    </section>\r\n</section>\r\n`\r\nclass RecordsContainer extends HTMLElement {\r\n    constructor() {\r\n        super();\r\n        this.root = this.attachShadow({\r\n            'mode': 'open'\r\n        });\r\n    }\r\n\r\n    connectedCallback() {\r\n        this.getInitialState()\r\n        this.render()\r\n    }\r\n\r\n    createRecord(payment) {\r\n        let record = document.createElement('paym-record')\r\n        record.setAttribute('title', payment.title)\r\n        record.setAttribute('amount', payment.amount)\r\n        record.setAttribute('category', payment.category)\r\n        record.setAttribute('date', payment.date)\r\n        record.setAttribute('comment', payment.comment)\r\n        return record\r\n    }\r\n\r\n    getInitialState() {\r\n        const payments = JSON.parse(this.getAttribute('payments'))\r\n\r\n        return {payments}\r\n    }\r\n\r\n    configure(node, state) {\r\n        let recordsFound = node.querySelector('#recordsFound')\r\n        let records = node.querySelector('#recordsList')\r\n        let sum = node.querySelector('#sumValue')\r\n\r\n        // Add payment nodes\r\n        state.payments.forEach(payment => {\r\n            let pmNode = this.createRecord(payment)\r\n            records.appendChild(pmNode)\r\n        })\r\n        \r\n        // Add sum value\r\n        let sumValue = state.payments.reduce((acc, curr) => {\r\n            return acc + curr.amount\r\n        }, 0)\r\n        sum.textContent = sumValue\r\n\r\n        recordsFound.textContent = state.payments.length\r\n    }\r\n\r\n    render() {\r\n        let node = template.content.cloneNode(true)\r\n        let state = this.getInitialState()\r\n        this.configure(node, state)\r\n\r\n        this.root.appendChild(node)\r\n\r\n    }\r\n\r\n}\r\n\r\nexport default RecordsContainer"},"lineCount":null}},"hash":"20073cb8e8e21620c4bad735f39f34d8","cacheData":{"env":{}}}